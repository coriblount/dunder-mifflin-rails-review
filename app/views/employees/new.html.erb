<h1> New employee form </h1>

<%= form_with model:@employee do |f| %>
<%= f.label :first_name%>
<%= f.text_field :first_name%><br>
<%= f.label :last_name%>
<%= f.text_field :last_name%><br>
<%= f.label :alias%>
<%= f.text_field :alias%><br>
<%= f.label :title%>
<%= f.text_field :title%><br>
<%= f.label :office%>
<%= f.text_field :office%><br>
<%= f.label :dog_id%>
<%= f.text_field :dog_id%><br>
<%= f.label :img_url%>
<%= f.text_field :img_url%><br>
<%= f.submit %>
<% end %>